<h1>Sign up</h1>
<form>
  <div class="form-group">
    <label for="name">Nombre</label>
    <input type="name" id="name" name="name" placeholder="name" autocomplete="on"><br>
  </div>
    <div class="form-group">
    <label for="lastname">Apellido</label>
    <input type="lastname" id="lastname" name="lastname" placeholder="lastname" autocomplete="on"><br>
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input id="password" name="password" type="password" placeholder="password"><br>
  </div>
  <div class="form-group">
    <label for="age">Edad</label>
    <input id="age" name="age" type="number" placeholder="age" autocomplete="on"><br>
  </div>
  <div class="form-group">
    <label for="email">Email</label>
    <input id="email" name="email" type="text" placeholder="email" autocomplete="on"><br>
  </div>
  <div class="form-group">
    <label for="rol">Rol</label>
    <input id="rol" name="rol" type="text" placeholder="rol" autocomplete="on"><br>
  </div>
  <button type="submit">Register</button>
</form>

<script src="/static/js/register.js"></script>